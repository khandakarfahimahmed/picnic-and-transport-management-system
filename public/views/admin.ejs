<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>resort</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link rel="stylesheet" href="../views/styleresort.css">
  </head>
    <nav class="navbar">
        <div class="container text-center">
        <h3 class="text-center">Admin Panel</h3>
        </div>
    </nav>

<body class="container">

    <div class="row card-columns  g-3">
        <div class="col">
            <div class="card">
                <div class=" card-header">
                    all resorts
                </div>

                <ul class="list-group ">
                    <% for(var i=0; i<resort_names.length; i++) {%>
                        <li class="list-group-item">
                            <%= resort_names[i]["resort_name"] %>
                        </li>
                        <% } %>

                </ul>
            </div>
        </div>

        <div class="col">
            <div class="card">
                <div class="card-header">
                    resorts booked
                </div>
                <ul class="list-group ">
                    <% for(var i=0; i<resorts_booked.length; i++) {%>
                        <li class="list-group-item">
                            <%= resorts_booked[i]["resort_name"] %>
                        </li>
                        <% } %>
                </ul>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <div class="card-header">
                    pay On Arrival
                </div>
                <ul class="list-group ">
                    <% for(var i=0; i<payOnArrival.length; i++) {%>
                        <li class="list-group-item">
                            <%= payOnArrival[i]["email"] %>
                        </li>
                        <% } %>
                </ul>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <div class="card-header">
                    bkash payment
                </div>
                <ul class="list-group ">
                    <% for(var i=0; i<bkashPayment.length; i++) {%>
                        <li class="list-group-item">
                            <%= bkashPayment[i]["email"] %>
                        </li>
                        <% } %>
                </ul>
            </div>
        </div>
        <div class="col">
            <div class="card">
                <div class="card-header">
                    transport payment history
                </div>
                <ul class="list-group ">
                    <% for(var i=0; i<transportPayment.length; i++) {%>
                        <li class="list-group-item">
                            <%= transportPayment[i]["username"] %>
                        </li>
                        <% } %>
                </ul>
            </div>
        </div>


    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
</body>


</html>